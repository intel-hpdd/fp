{
  "name": "intel-fp",
  "version": "4.1.2",
  "description": "A Functional Programming Lib.",
  "main": "dist/fp.js",
  "scripts": {
    "test:dev": "npm run prepublish && npm t",
    "test": "NODE_ENV=test ./bin/test.js",
    "cover": "NODE_ENV=test istanbul cover --report cobertura --include-all-sources ./bin/test.js",
    "eslint": "eslint ./",
    "jscs": "jscs .",
    "prepublish": "rm -rf dist && babel -d dist fp.js test/*.js && cp fp.js dist/fp.js.flow"
  },
  "publishConfig": {
    "registry": "https://ubit-artifactory-or.intel.com/artifactory/api/npm/iml-npm-local"
  },
  "pre-commit": [
    "jscs",
    "eslint",
    "test:dev"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.intel.com/IML/fp.git"
  },
  "author": "IML Team",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-core": "6.5.1",
    "babel-eslint": "5.0.0-beta10",
    "babel-plugin-transform-flow-strip-types": "6.5.0",
    "babel-plugin-transform-strict-mode": "6.5.0",
    "babel-preset-es2015": "6.5.0",
    "eslint": "1.10.3",
    "eslint-plugin-flow-vars": "0.1.3",
    "immutable": "3.7.6",
    "intel-jasmine-n-matchers": "2.1.1",
    "istanbul": "0.4.2",
    "jasmine": "2.4.1",
    "jscs": "2.9.0",
    "intel-jasmine-junit-reporter": "1.0.0",
    "pre-commit": "1.1.2"
  }
}
